{
  "name": "labelcopier-core",
  "version": "0.0.0",
  "description": "Business logic of Labelcopier",
  "main": "index.js",
  "repository": "https://github.com/BadwaterBay/labelcopier-core.git",
  "author": "Badwater Bay <badwaterbay@gmail.com>",
  "license": "BSD-3-Clause",
  "scripts": {
    "format": "prettier '**/*.{js,ts,json}' --write",
    "lint": "eslint '**/*.{js,ts}' --cache --fix",
    "test": "mocha --config .mocharc.js -r @babel/register src/test/**/*.{spec,test}.js",
    "coverage": "nyc yarn test"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ],
    "plugins": [
      "istanbul"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,json}": [
      "prettier --write"
    ],
    "**/*.{js,ts}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "mocha": "^8.1.3",
    "nock": "^13.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "regenerator-runtime": "^0.13.7"
  }
}
